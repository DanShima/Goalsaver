<?xml version="1.0" encoding="utf-8"?>
<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable name="saving" type="com.danshima.savemyq.model.SavingsRule"/>
    </data>

    <ImageView
        android:id="@+id/tab_rule_item"
        android:layout_margin="4dp"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:padding="4dp"
        android:contentDescription="@null"
        android:layout_marginStart="@dimen/standard8"
        android:background="@drawable/ic_round_up"
        app:bindSavingRulesIcon="@{saving}"/>
</layout>